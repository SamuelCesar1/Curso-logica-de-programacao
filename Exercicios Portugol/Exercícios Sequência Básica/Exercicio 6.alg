algoritmo "Exercicio"
//Ex:6)    Faça um programa que leia um número inteiro e mostre o seu antecessor e seu
//         sucessor.
var
num, ant, suc: inteiro
inicio

      Escreva("Informe um número inteiro: ")
      Leia (num)
      ant <- num - 1
      suc <- num + 1
      Escreval("O Antecessor de ",num," é: ",ant)
      Escreval("O Sucessor de ",num," é: ",suc)
fimalgoritmo
