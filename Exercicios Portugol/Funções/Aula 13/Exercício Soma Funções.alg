algoritmo "semnome"

var
V1, V2,S:inteiro
Funcao Soma(n1,n2:inteiro):inteiro
inicio
      Retorne n1+n2
FimFuncao
inicio
      Escreva("Informe o primeiro valor: ")
      Leia(V1)
      Escreva("Informe o segundo valor: ")
      Leia(V2)
      S <- Soma(V1,V2)
      Escreval("O valor da soma de",V1," e",V2," é",S)
fimalgoritmo
