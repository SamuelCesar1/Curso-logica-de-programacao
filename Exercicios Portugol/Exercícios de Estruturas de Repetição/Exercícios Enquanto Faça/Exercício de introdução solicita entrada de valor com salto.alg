algoritmo "EstruturaEnquanto"

var
I,cont,salto:inteiro
inicio
      Escreva("Informe até quando o programa deve contar? ")
      Leia(cont)
      Escreva("Qual será o valor do salto? ")
      Leia(salto)
      I <- 0
      Enquanto (I <= cont) faca
               Escreval(I)
               I <- I + salto
      FimEnquanto
      Escreval("Terminei de contar")
fimalgoritmo
