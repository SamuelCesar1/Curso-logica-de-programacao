algoritmo "EstruturaEnquanto"

var
I,cont:inteiro
inicio
      Escreva("Informe até quando o programa deve contar? ")
      Leia(cont)
      I <- 0
      Enquanto (I <= cont) faca
               Escreval(I)
               I <- I + 1
      FimEnquanto
      Escreval("Terminei de contar")
fimalgoritmo
