algoritmo "semnome"

var
N: inteiro
Procedimento ParOuImpar(V:inteiro)
Inicio
      Se (V % 2 = 0) entao
         Escreval("O Número é Par!!")
      SeNao
           Escreval("O Número é Impar!!")
      FimSe
FimProcedimento
inicio
      Escreva("Digitre um número: ")
      Leia(N)
      ParOuImpar(N)
fimalgoritmo
